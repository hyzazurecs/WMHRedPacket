/**app.wxss**/
@import './lib/style/font.wxss';
.container {
  height: 100%; 
  width:100%;
  font-family:"PingFang SC Medium","微软雅黑";
  font-size:12px;
  color:#3e3e3e;
} 
.con{
  overflow: hidden;
  width:100%;
}
.fr{
  float:right;
}
.fl{
  float:left;
}
.overF{
  overflow: hidden;
}
.header_logo{
  width:110rpx;
  height:110rpx;
  border:15rpx;
  margin:15rpx auto;
}
.header{
  width:110rpx;
  height:110rpx;
  border:10rpx;
  border-radius:50%;
}
.header_Head{
  width:150rpx;
  height:150rpx;
  border-radius:50%;
  border:8rpx solid #fff;
}
.marginAuto{
  margin: 0 auto;
}
.qingting{
  width:600rpx;
  height:60rpx;
  line-height:60rpx;
  background:#f3f5fa;
  font-size:20rpx;
  color:#999;
  border-radius:15rpx;
}
.qingTing{
  width:530rpx;
  height:64rpx;
  background:#fff;
  line-height: 64rpx;
  color:#3e3e3e;
  font-size:24rpx;
  margin-top:30rpx;
  border-radius:15rpx;
}
.paddLeft25{
  padding-left:25rpx;
}
.paddRight25{
  padding-right:25rpx;
}
.paddLeft15{
  padding-left:15rpx;
}
.paddRight15{
  padding-right:15rpx;
}
.margLeft25{
  margin-left:25rpx;
}
.margRight25{
  margin-right:25rpx;
}
.marginTop25{
  margin-top:25rpx;
}
.margLeft15{
  margin-left:15rpx;
}
.margRight15{
  margin-right:15rpx;
}

